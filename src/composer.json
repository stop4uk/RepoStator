{
    "name": "stop4uk/repostator",
    "description": "RMS, RMS Repostator, Reports & Statistics. Make any report easy. A system for receiving data and generating reports",
    "keywords": ["stop4uk", "repostator", "report", "generate report", "statistic", "getting data", "rms"],
    "homepage": "https://www.repostator.github.io",
    "type": "project",
    "support": {
        "issues": "https://github.com/stop4uk/repostator/issues?state=open",
        "source": "https://github.com/stop4uk/repostator"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "yiisoft/yii2": "~2.0.51",
        "yiisoft/yii2-bootstrap5": "~2.0.2",
        "yiisoft/yii2-symfonymailer": "~2.0.3",
        "yiisoft/yii2-queue": "^2.3",
        "yii2tech/ar-softdelete": "^1.0",
        "kartik-v/yii2-widget-select2": "^2.2",
        "kartik-v/yii2-date-range": "^1.7",
        "kartik-v/yii2-widget-datepicker": "^1.4",
        "twbs/bootstrap-icons": "^1.10",
        "klisl/yii2-nested-sets-drag-and-drop": "^1.3",
        "phpoffice/phpspreadsheet": "^2.0",
        "yiithings/yii2-dotenv": "^1.1",
        "creocoder/yii2-flysystem": "^1.2",
        "2amigos/yii2-file-upload-widget": "dev-master",
        "marqu3s/yii2-summernote": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "insolita/yii2-migration-generator": "^3.2",
        "symfony/browser-kit": "*",
        "elisdn/yii2-gii-fixture-generator": "^1.1",
        "codeception/codeception": "*",
        "codeception/module-asserts": "*",
        "codeception/module-yii2": "^1.1.10",
        "codeception/module-filesystem": "*",
        "codeception/verify": "*",
        "codeception/module-rest": "*",
        "codeception/module-phpbrowser": "*",
        "codeception/module-webdriver": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "app\\": "application"
        }
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/lightinen/yii2-file-upload-widget.git"
        }
    ]
}
